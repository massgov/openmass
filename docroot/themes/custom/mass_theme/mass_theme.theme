function mass_theme_preprocess_node_regulation(&$variables) {
  $node = $variables['node'];
  $category = 'Regulation';
  $publish_state = NULL;
  $cache_tags = [];
  $listingTable = [];
  $sideContent = [];

  // Get the page title for title context.
  $page_title = $node->getTitle();

