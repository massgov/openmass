{#
/**
 * @file
 * Display the Logo and Link view mode of the Org page content type.
 */
 #}
{% set image = null %}
{% if node.field_org_page_thumbnail is not empty %}
  {% set image = {
    src: file_url(node.field_org_page_thumbnail.entity.fileuri ?? ''),
    alt: node.field_org_page_thumbnail.alt,
  } %}
{% endif %}
{{ include('@molecules/image-link.twig', {
    imageLink: {
      block: null,
      href: url,
      info: label|field_value,
      image: image,
      text: label,
    },
  },
) }}
