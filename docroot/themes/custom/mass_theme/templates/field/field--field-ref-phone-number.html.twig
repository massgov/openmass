{#
/**
 * @file
 * Theme implementation for the Ref Phone Number field.
 */
#}

{# Set top heading level for the component. #}
{% set level = 4 %}
{% if element['#level'] is not empty %}
  {% set level = element['#level'] %}
{% endif %}

{% embed "@molecules/contact/contact-group.twig" with {
  "contactGroup": {
    "name": "Phone",
    "icon": "phone",
    "level": level
  }
} %}
  {% block groupItems %}
    {% for item in items %}
      {{ item.content }}
    {% endfor %}
  {% endblock %}
{% endembed %}
